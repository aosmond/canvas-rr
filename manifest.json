{
   "manifest_version": 2,
   "name": "canvas-rr",
   "version": "0.3",

   "content_scripts": [
      {
        "matches": ["https://*/*"],
        "js": ["inject.js"],
        "run_at": "document_start",
        "all_frames": true
      }
   ],


   "web_accessible_resources": [
      "record.js"
   ]
}
