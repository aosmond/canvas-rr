{
   "manifest_version": 2,
   "name": "canvas-rr",
   "version": "0.4",

   "content_scripts": [
      {
        "matches": ["https://*/*", "http://*/*", "file://*/*"],
        "js": ["out/rr-record.content.js"],
        "run_at": "document_start",
        "all_frames": true
      }
   ],

   "web_accessible_resources": [
      "rr-record.js"
   ]
}
